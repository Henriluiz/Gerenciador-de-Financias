{
  "expo": {
    "name": "Picpay",
    "slug": "financie",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/picpay.png",
    "backgroundColor": "#21C25E",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/picpay.png",
      "resizeMode": "contain",
      "backgroundColor": "#21C25E"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/picpay.png",
        "backgroundColor": "#21C25E"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.luizhpzin.financie"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "00de6d90-c545-482f-a1fa-828a21b35829"
      }
    },
    "plugins": [
      "expo-secure-store"
    ]
  }
}
